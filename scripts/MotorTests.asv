figure;

title('Velocidade');

xlabel('Ua [V]');

ylabel('v [m/s]');

hold on;



u = [2,0.1,6.5];
u_range = u(1):u(2):u(3);


%Motor 2
Ra = 7.5000;
kf = 0.6741;
kt = 0.6613;


%Simulação steady-state do motor
[v, T, I] = MotorSSsim(u, Ra, kf, kt);


%Gráficos
figure;
subplot(
plot(u_range, v);
plot(u_range, T,'.');
plot(u_range, I,'*');
figure;
plot(u_range, u_range.*I','+');